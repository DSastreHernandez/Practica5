
public class Calculadora implements ICalculadora{
	private int a;
	private int b;
	
	public Calculadora(int a, int b) {
		this.a = a;
		this.b = b;
	}
	
	@Override
	public int sumar(int a, int b) {
		return a+b;
	}
	
	@Override
	public int restar(int a, int b) {
		return a-b;
	}
	
	@Override
	public int multiplicar(int a, int b) {
		return a*b;
	}
	
	@Override
	public int dividir(int a, int b) {
		return a/b;
	}
}
